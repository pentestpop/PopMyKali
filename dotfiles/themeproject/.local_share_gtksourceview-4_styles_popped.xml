<?xml version="1.0" encoding="UTF-8"?>
<style-scheme id="popped" name="Popped" version="1.0">
  <author>Gemini</author>
  <description>Living that Pop life</description>

  <style name="text" foreground="#F6EFEE" background="#010A21"/>
  <style name="selection" background="#1A3A5F" foreground="#ADE7FF"/>
  <style name="cursor" foreground="#F9629F"/>
  <style name="current-line" background="#101935"/>
  <style name="line-numbers" foreground="#4B6A88" background="#010A21"/>
  <style name="current-line-number" foreground="#F6EFEE" background="#101935" bold="true"/>
  
  <style name="bracket-match" background="#5167C4" foreground="#F6EFEE"/>
  <style name="bracket-mismatch" background="#D24D7D" foreground="#F6EFEE"/>

  <style name="search-match" background="#D9A441" foreground="#010A21"/>

  <style name="def:comment" foreground="#4B6A88" italic="true"/>
  <style name="def:shebang" foreground="#4B6A88" italic="true"/>
  <style name="def:doc-comment-element" foreground="#4B6A88" italic="true"/>

  <style name="def:string" foreground="#70C2A3"/>
  <style name="def:special-char" foreground="#00B7D4"/>
  
  <style name="def:keyword" foreground="#FC9CC2" bold="true"/>
  <style name="def:statement" foreground="#FC9CC2"/>
  <style name="def:type" foreground="#00B7D4"/>
  <style name="def:builtin" foreground="#ADE7FF"/>
  
  <style name="def:number" foreground="#EFD6AC"/>
  <style name="def:decimal" foreground="#EFD6AC"/>
  <style name="def:base-n-integer" foreground="#EFD6AC"/>
  <style name="def:floating-point" foreground="#EFD6AC"/>
  <style name="def:complex" foreground="#EFD6AC"/>
  <style name="def:boolean" foreground="#EFD6AC"/>
  <style name="def:constant" foreground="#FC9CC2"/>
  
  <style name="def:function" foreground="#EFD6AC"/>
  <style name="def:identifier" foreground="#ADE7FF"/>
  
  <style name="def:operator" foreground="#DDCBBD"/>
  <style name="def:special-constant" foreground="#F9629F"/>
  <style name="def:preprocessor" foreground="#D24D7D"/>
  
  <style name="def:error" foreground="#F9629F" underline="true" bold="true"/>
  <style name="def:warning" foreground="#D9A441" underline="true"/>
  <style name="def:note" foreground="#70C2A3"/>

</style-scheme>
